{
  "functions": {
    "source": "functions"
  },
  "emulators": {
    "functions": {
      "port": 5001
    },
    "storage": {
      "port": 9199
    },
    "ui": {
      "enabled": true
    },
    "hosting": {
      "port": 5000,
      "host": "0.0.0.0"
    }
  },
  "storage": {
    "rules": "storage.rules"
  },
  "hosting": {
    "public": "example",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  }
}
